expr:Double
  rdf:type owl:Class ;
  rdfs:subClassOf expr:Primitive ;
  owl:equivalentClass [
      rdf:type owl:Restriction ;
      owl:hasValue expr:t_double ;
      owl:onProperty expr:hasDataType ;
    ] ;
